@article{Chene2005,
	author = {Chene, Dennis Des},
	category = {Secondary},
	date = {2005},
	date-added = {2015-09-09 17:10:49 +0000},
	date-modified = {2022-12-24 11:57:44 -0500},
	journal = {Studies in History and Philosophy of Biology andBiomedical Sciences},
	keywords = {Giovanni Alfonso Borelli; Charles Perrault; Pierre Sylvain Regis; Mechanism; Physiology},
	number = {2},
	pages = {245--260},
	title = {Mechanisms of Life in the Seventeenth Century: Borelli, Perrault, Régis},
	volume = {36},
	abstract = {In Descartes\&rsquo;s reformulation of natural philosophy, two aspects of what came to be known as the mechanical philosophy were intimately joined: mechanism as an ontology of nature, according to which all natural things had only &lsquo;mechanical&rsquo; properties; and mechanism as a method of explanation. One could, and many philosophers did, adopt mechanism as a method of explanation without adopting a mechanistic ontology. I examine two successors of Descartes who did just that, and one who did not. Giovanni Alfonso Borelli in his <i>De motu animalium</i> (1680) and Charles Perrault in his <i>M&eacute;canique de animaux</i> (1680&ndash;1688, Vol. 3) propose and argue for a variety of mechanical accounts of the operations of animals, in particular of their muscles. They reject, however, the Cartesian reduction of animal souls to mechanical forces; the principle of animal motion for them remains a non-mechanical soul. Pierre Sylvain R&eacute;gis in his <i>Cours entier de philosophie</i> (1690, 1691) follows Descartes in taking the &lsquo;physical cause&rsquo; of motion in animals to be the fermentation of blood in the heart, and thus denies animal souls any role in his physiology. That he can do so while taking over, sometimes word for word, Perrault&rsquo;s accounts of animal motion shows that mechanistic explanation and mechanistic ontology could easily part company. },
	bdsk-file-1 = {},
	bdsk-url-1 = {https://doi.org/10.1016/j.shpsc.2005.03.002}}

