@article{Marshall2013,
	author = {Marshall, Douglas Bertrand},
	category = {Secondary},
	date = {2013},
	date-added = {2015-09-09 17:10:49 +0000},
	date-modified = {2022-12-24 11:57:25 -0500},
	journal = {Studies in History and Philosophy of Science},
	number = {2},
	pages = {178--187},
	title = {Galileo's defense of the application of geometry to physics in the <i>Dialogue</i>},
	volume = {44},
	abstract = {Alessandro Piccolomini and several other prominent 16th-century Aristotelians claimed that while a sphere touches a plane at a point in geometry, a material sphere touches a plane not at a point but over a small surface. These thinkers thereby called into question the reliability of geometric reasoning in physics. In this article I provide a detailed analysis of Galileo&rsquo;s reply to such worries about geometry in the Second Day of his <i>Dialogue Concerning the Two Chief World Systems</i>. Because his infamous extrusion argument relies on the premise that a sphere touches a plane at a point, Galileo takes the opportunity to defend the argument and to attack geometry&rsquo;s critics. According to the account I propose, Galileo argues for the applicability of geometry to physics by defending the legitimacy of geometric approximations and by advocating an expansive notion of geometric curve. },
	bdsk-file-1 = {},
	bdsk-url-1 = {https://doi.org/10.1016/j.shpsa.2013.02.001}}

