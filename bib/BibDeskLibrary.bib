@article{BienerSchliesser2017,
	author = {Biener, Zvi and Schliesser, Eric},
	category = {Secondary},
	date = {2017},
	date-added = {2019-03-06 11:37:34 -0500},
	date-modified = {2022-12-24 11:58:23 -0500},
	journal = {Monist},
	month = {July},
	pages = {311-325},
	title = {The Certainty, Modality, and Grounding of {Newton}'s Laws},
	volume = {100},
	abstract = {Newton began his *Principia* with three Axiomata sive Leges Motus. We offer an interpretation of Newton’s dual label and investigate two tensions inherent in his account of laws. The first arises from the juxtaposition of Newton’s confidence in the certainty of his laws and his commitment to their variability and contingency. The second arises because Newton ascribes fundamental status both to the laws and to the bodies and forces they govern. We argue the first is resolvable, but the second is not. However, the second tension shows that Newton conceives laws as formal causes of bodies and forces. This neo-Aristotelian conception goes missing in Kantian accounts of laws, as well as accounts that stress laws’ grounding in powers and capacities.},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://doi.org/10.1093/monist/onx012}}

