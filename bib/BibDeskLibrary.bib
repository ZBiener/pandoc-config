@article{McKirahan1978,
	author = {McKirahan, Richard D.},
	category = {Secondary},
	date = {1978},
	date-added = {2015-09-09 17:10:49 +0000},
	date-modified = {2022-12-24 11:57:00 -0500},
	journal = {British Journal for the History of Science},
	keywords = {Aristotle, Mixed Math, Posterior Analytics, Physics, Zabarella, Ancient, Optics, Euclid},
	pages = {197--220},
	title = {Aristotle's Subordinate Sciences},
	volume = {11},
	annote = {Work in the mixed science tradition beginning with pseudo-Euclid and    Archimedes is an instance of the theory of science of the    \emph{Posterior Analytics}. The crux of the theory is that 1) a    subordinate science depends on the superior for its proofs and    principles, 2) the subordinate science treates knowledge of the fact,    the superior of the reasoned fact, 3) subordinate sciences are    perceptual, superior are mathematical. McKirahan analyses the examples    of optics/geometry and harmonics/arithmatics and question cases of    triple suordinate sciences, such as arithmatics/mathematical    harmonics/acoustical harmonics. The division corresponds to something    like pure, applied, and experimental science, although McKirahan doesn't    use this language. He then analyses the 3 theses in relation to each    pair in the triple, concluding that they only hold with respect to    certain pairs and with certain qualifications. The argument seems mostly    right. The main problem seems to me that``fact gathering" is difficult    to fit into the subordinate science. Open avenues of investigation    should be whether Aristotle on fact gathering provides further evidence,    and whether a moden conception of experimental sciences can help out    Aristotle. However, a careful study with lots of references. Oh,    McKirahan self-consciouly mirror Zabarella's treatment of the mixed    sciences!! Following Zabarella, he introduces a nice turn of phrase to    indicate a form of treatment (i.e., geometrical treatment): \emph{modus    considerandi}.},
	bdsk-file-1 = {}}

