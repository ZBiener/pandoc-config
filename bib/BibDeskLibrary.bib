@article{Garber1982,
	author = {Garber, Daniel},
	category = {Secondary},
	date = {1982},
	date-modified = {2022-12-23 04:22:18 -0500},
	journal = {Archiv f\"{u}r Geschichte der Philosophie},
	keywords = {Descartes, Synthesis,},
	note = {Reprinted in \cite{ZGarb01}, Chap. 3, pp. 52--63},
	pages = {136--47},
	title = {A Point of Order: Analysis, Synthesis and {Descartes}' \emph{Principles}},
	volume = {64},
	annote = {The conversations with Burman (and previously held common wisdom) have it that the Principles are a synthetic presentation of the Meditation, which are analytic in nature. Descartes' distinction between analytic/synthetic are offered in the Second Replies. Gueroult interprets this distinction as corresponding to the one in the Regulae between the order of knowing and the order of being. However, Gueroult interpretation does not make sense of the texts. The Principles and the Meditation are alike in overall order: both begin with doubt, move to god, and continue to the natural world.

\par Edwin Curley begins offers a different interpretation, starting with the idea that we can learn most about Descartes' use of synthesis by looking comparing the Second Replies and the Principles. Certain similarities can be found: use of definitions, formal principles, and the lack of heuristic, false-start arguments that are used in the Meditations.

\par But neither of these approaches establish that Descartes really saw the Principles as a synthetic work! In fact, Descartes could have refered to the Principles as synthetic on many occasions, but he did not. Garber lists such occasions. Ultimately, Garber concludes that the analytic/synthetic distinction does not matter for understanding the Principles. The argument is convincing, although not air-tight.}}

@article{Garber1987,
	author = {Garber, Daniel},
	category = {Secondary},
