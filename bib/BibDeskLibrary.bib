@incollection{Shea1978,
	address = {Dordrecht; Boston},
	author = {Shea, William R.},
	booktitle = {New perspectives on Galileo},
	category = {Secondary},
	date = {1978},
	date-modified = {2022-12-23 04:22:14 -0500},
	editor = {Butts, Robert E. and Pitt, Joseph C.},
	keywords = {Galileo, Descartes, Mechanics},
	pages = {139--159},
	publisher = {D. Reidel},
	title = {{Descartes} as Critic of Galileo},
	annote = {Descartes and Galileo never made any contact. Galileo was not aware of    Descartes' work. Descartes responded to Galileo's condemnation, but    without reading the \emph{Dialogue}. Beeckman bring the Dialogue to    Descartes in 1634, but takes it back with him after a few days (he did    get a copy of the \emph{Discorsi} upon publication) . Descartes reports    favorably on the book to Mersenne, but is not impressed with the tides.    HOWEVER, Descartes claims he arrived at the law of free-fall    independently. Shea shows that he did not, he continuously uses an    improper rule, which correlated force with speed (not acceleration) and    assumed times are proportional to distances. However, ``around 1630,    Descartes' understanding of physics underwent a profound change that led    him to see the solution to the problem of free fall in an entirely    different light and eventually to conclude that it was mathematically    intractable'' (p. 146). The change began in the \emph{Regulae}, and its    fruits embodied in the \emph{Discourse}. For example, he writes to    Mersenne in 1637 that the ration of free fall ``is something that    depends on so many others that I could not give an dequate account in a    letter. All I can say is that neither Galileo nor anyone else can    determine anything concerning this that is clear and dmeonstrative if he    does not first know what weight is and does not have the true principles    of physics.''\\Descartes read the \emph{Discorsi}, but was dismissive.    However, he applauded Galileo's work on music, which he still claimed he    arrived at first. (In general, Descartes stresses that he did not steal    anything from Galileo). Shea lists Descartes' criticisms, which are    leveled at Aristotle's Wheel and an assortment of others. Descartes also    criticizes Mersenne's version of Galileo's mechanics as showing the how,    not the why, of simple machines.}}

